[
  {
    "type": "console.page/route",
    "properties": {
      "exact": false,
      "path": "/camel/application/ns/:ns/kind/:kind/name/:name",
      "component": { "$codeRef": "CamelApplication" }
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": ["/camel/ns/:ns", "/camel/all-namespaces"],
      "component": { "$codeRef": "CamelApplicationList" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "camel",
      "name": "%plugin__camel-openshift-console-plugin~Camel%",
      "href": "/camel",
      "perspective": "dev",
      "section": "resources",
      "namespaced": true
    }
  }
]
